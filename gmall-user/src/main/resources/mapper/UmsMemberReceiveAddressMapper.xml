<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.atguigu.gmall.gmalluser.mapper.UmsMemberReceiveAddressMapper">
    <select id="getMemberId" resultType="com.atguigu.gmall.gmalluser.bean.UmsMemberReceiveAddress">
        select d.* from ums_member_receive_address d where d.member_id=#{memberId}
    </select>

    <select id="selectMember" resultType="com.atguigu.gmall.gmalluser.bean.UmsMemberReceiveAddress">
        select * from ums_member_receive_address
    </select>



</mapper>